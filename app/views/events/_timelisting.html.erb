<table class = 'table'>
	<tr>
		<th>Time</th>
		<th>Friend</th>
	</tr>
<% times.each do |timelisting| %>
  <tr>
    <td><%= timelisting.startHour %> to <%= timelisting.endHour %></td>
    <td><%= User.find(timelisting.user_id).name %></td>
    <% if timelisting.user_id == current_user.id %>
	    <td><%= link_to "Delete", timelisting,
	    	 :method => :delete, :remote => true, :class => 'btn' %></td>
	<% end %>
  </tr>
<% end %>
</table>
